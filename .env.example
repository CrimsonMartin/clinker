# Firebase Configuration - Example Template
# Copy this file to .env and replace with your actual Firebase project values
# Get these values from: Firebase Console > Project Settings > Your apps > Web app config

FIREBASE_API_KEY=your_actual_api_key_here
FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id_here
FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
FIREBASE_APP_ID=your_app_id_here

# Donation Configuration
DONATION_URL=https://www.buymeacoffee.com/YOUR_USERNAME
